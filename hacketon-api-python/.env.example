# Configurações da API Python
PORT=5000
FLASK_DEBUG=true

# Modo de operação: 'true' para dados live, 'false' para dados mock
USE_LIVE_DATA=false

# Credenciais NASA (opcionais - usar .netrc é recomendado)
# NASA_USERNAME=seu_usuario
# NASA_PASSWORD=sua_senha
# EARTHDATA_TOKEN=seu_token

# Configurações de cache
CACHE_DURATION=300

# Configurações de logging
LOG_LEVEL=INFO